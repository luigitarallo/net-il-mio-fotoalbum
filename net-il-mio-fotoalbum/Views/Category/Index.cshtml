@model List<Category>

<div class="wrapper">
    <div class="container">
        <h1 class="text-center text-light p-4">All Photos</h1>
        <div class="row">
            <table class="table table-dark table-bordered">
                <thead>
                    <tr>
                        <th scope="col" class="text-center bg-danger">Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (Category category in Model)
                    {
                        <tr>
                            <td class="text-center align-content-center">@category.Name</td>
                        </tr>
                    }
                </tbody>
                <div class="d-flex justify-content-end">
                    <button type="button" class="btn btn-outline-danger mb-5">
                        <a href="@Url.Action("Create","Category")">Add New Category</a>
                    </button>
                </div>
            </table>
        </div>

